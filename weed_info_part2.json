    "te": {
        "Ambrosia_artemisiifolia": {
            "characteristics": [
                "వార్షిక మొక్క 2 మీటర్ల పొడవు వరకు పెరుగుతుంది",
                "నిటారుగా, కొమ్మలతో కూడిన కాండాలు",
                "లోతైన లోబ్స్, ఫెర్న్ వంటి ఆకులు",
                "చిన్న ఆకుపచ్చ పువ్వులు",
                "విస్తృతమైన వేరు వ్యవస్థ",
                "అనేక గాలి ద్వారా వ్యాపించే విత్తనాలను ఉత్పత్తి చేస్తుంది",
                "అత్యంత అలెర్జీకరమైన పుప్పొడి",
                "వర్షపాతం తట్టుకోగలదు",
                "అశాంత ప్రాంతాల్లో బాగా పెరుగుతుంది",
                "పంటలతో పోటీపడుతుంది",
                "దట్టమైన స్టాండ్లను ఏర్పరుస్తుంది",
                "నియంత్రించడం కష్టం",
                "వివిధ నేలలకు అనుకూలంగా ఉంటుంది",
                "విత్తనం ద్వారా వ్యాపిస్తుంది",
                "వేడి పరిస్థితుల్లో మొలకెత్తుతుంది"
            ],
            "control_methods": {
                "cultural": [
                    "స్వచ్ఛమైన పంట విత్తనాలను ఉపయోగించండి",
                    "పంట భ్రమణం అభ్యాసం చేయండి",
                    "దట్టమైన పంట స్టాండ్లను నిర్వహించండి",
                    "సరైన క్షేత్ర వ్యవస్థాపనను అమలు చేయండి",
                    "పోటీ పంట వైవిధ్యాలను ఉపయోగించండి"
                ],
                "mechanical": [
                    "ప్రారంభ దశలో చేతితో కలుపు తీయడం",
                    "నియమిత సాగు",
                    "పుష్పించే ముందు కోయడం",
                    "లోతైన దున్నడం",
                    "పుప్పొడి విడుదలకు ముందు మొక్కలను తొలగించండి"
                ],
                "chemical": [
                    "ముందు-ఉద్భవం: అట్రాజిన్",
                    "తర్వాత-ఉద్భవం: గ్లైఫోసేట్",
                    "ఎంపిక: 2,4-D",
                    "ట్యాంక్ మిక్స్: డికాంబా + 2,4-D",
                    "అవశేష: ఇమాజాపిక్"
                ],
                "biological": [
                    "పోటీ కవర్ పంటలు",
                    "సరైన పంట అంతరం",
                    "పంట భ్రమణం",
                    "సమగ్ర విధానం"
                ]
            }
        },
