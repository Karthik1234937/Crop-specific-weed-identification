    "te": {
        "Crabgrass": {
            "characteristics": [
                "సంవత్సరాంతం గడ్డి 1 మీటర్ ఉయరమ్ వరకు పెరుగుతుంది",
                "పరవ్యాప్తి చెందే, నేలపై పడి ఉండే పెరుగుదల",
                "తట్టుయాన మధ్యస్థ నరంతో ఫ్లాట్, విశాలమైన ఆకులు",
                "వేళ్ల వంటి విత్తన తలలు",
                "ఫైబరస్ రూట్ సిస్టమ్",
                "అధిక కరువు సహనశక్తి",
                "వేడి పరిస్థితుల్లో బాగా పెరుగుతుంది",
                "వేగవంతమైన పెరుగుదల రేటు",
                "సమృద్ధిగా విత్తనాలను ఉత్పత్తి చేస్తుంది",
                "దట్టమైన మాట్స్ ఏర్పరుస్తుంది",
                "పోషకాల కోసం పంటలతో పోటీపడుతుంది",
                "స్థాపించబడిన తర్వాత నియంత్రించడం కష్టం",
                "వివిధ నేల రకాలకు అనుగుణంగా మారుతుంది",
                "విత్తనాలు మరియు స్టోలన్స్ ద్వారా వ్యాపిస్తుంది",
                "వేడి పరిస్థితుల్లో మొలకెత్తుతుంది"
            ],
            "control_methods": {
                "cultural": [
                    "శుభ్రమైన నాటే పదార్థాన్ని ఉపయోగించండి",
                    "నియమిత పంట మార్పిడిని అభ్యసించండి",
                    "దట్టమైన పంట కానోపీని నిర్వహించండి",
                    "సరైన క్షేత్ర శుభ్రతను అమలు చేయండి",
                    "పోటీ పంట రకాలను ఉపయోగించండి"
                ],
                "mechanical": [
                    "ప్రారంభ దశలో చేతితో లాగడం",
                    "నియమిత దున్నడం",
                    "విత్తన ఏర్పాటుకు ముందు కత్తిరించడం",
                    "లోతైన దున్నడం",
                    "పుష్పించే ముందు మొక్కలను తొలగించండి"
                ],
                "chemical": [
                    "ప్రీ-ఎమర్జెన్స్: పెండిమెథాలిన్",
                    "పోస్ట్-ఎమర్జెన్స్: గ్లైఫోసేట్",
                    "సెలెక్టివ్: క్విన్క్లోరాక్",
                    "ట్యాంక్ మిక్స్: 2,4-D + డికామ్బా",
                    "రెసిడ్యువల్: అట్రాజిన్"
                ],
                "biological": [
                    "పోటీ కవర్ పంటలు",
                    "సరైన మొక్కల మధ్య దూరం",
                    "పంట మార్పిడి",
                    "సమగ్ర నిర్వహణ"
                ]
            }
        },
